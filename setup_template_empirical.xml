<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE experiments SYSTEM "behaviorspace.dtd">
<experiments>
  <experiment name="experiment1" repetitions="1" runMetricsEveryStep="true">
    <setup>setup</setup>
    <go>go</go>
    <timeLimit steps="{time_limit}"/>
    <metric>mean [Antiguedad-infra_Ab] of agebs</metric>
    <enumeratedValueSet variable="Eficiencia_NuevaInfra">
      <value value="{eficiencia_nuevainfra}"/>
    </enumeratedValueSet>
    <enumeratedValueSet variable="Eficiencia_Mantenimiento">
      <value value="{eficiencia_mantenimiento}"/>
    </enumeratedValueSet>
    <enumeratedValueSet variable="lambda">
      <value value="{lambda}"/>
    </enumeratedValueSet>
    <enumeratedValueSet variable="escala">
      <value value="&quot;{escala}&quot;"/>
    </enumeratedValueSet>
    <enumeratedValueSet variable="factor_subsidencia">
      <value value="{factor_subsidencia}"/>
    </enumeratedValueSet>
    <enumeratedValueSet variable="Recursos_para_distribucion">
      <value value="{recursos_para_distribucion}"/>
    </enumeratedValueSet>
    <enumeratedValueSet variable="recursos_para_mantenimiento">
      <value value="{recursos_para_mantenimiento}"/>
    </enumeratedValueSet>
    <enumeratedValueSet variable="recursos_nuevaInfrastructura">
      <value value="{recursos_nuevainfrastructura}"/>
    </enumeratedValueSet>
    <enumeratedValueSet variable="ANP">
      <value value="{ANP}"/>
    </enumeratedValueSet>
    <enumeratedValueSet variable="Requerimiento_deAgua">
      <value value="{requerimiento_deagua}"/>
    </enumeratedValueSet>
  </experiment>
</experiments>
